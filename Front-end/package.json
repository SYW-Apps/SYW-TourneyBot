{
  "name": "syw-react-app-template",
  "version": "1.0.0",
  "description": "A React Web App template by SYW.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "mv.net-react": "xcopy dist ..\\wwwroot /s/e/Y",
    "build.net-react": "npm run build && npm run mv.net-react",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "cy:run": "cypress run --browser=chrome",
    "cy:run-electron": "cypress run --browser=electron",
    "cy:ci": "cypress run",
    "start-test-server": "set PORT=3000 && set HTTPS=false && set VITE_APP_ENVIRONMENT=development && npm start",
    "cy:test": "start-server-and-test \"npm run start-test-server\" http://localhost:3000 cy:run-electron",
    "cy:verify": "cypress verify",
    "cy:info": "cypress info"
  },
  "dependencies": {
    "@emotion/styled": "^11.11.0",
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-brands-svg-icons": "^6.4.2",
    "@fortawesome/free-regular-svg-icons": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@mui/material": "^5.14.20",
    "@mui/x-date-pickers": "^6.12.1",
    "@types/eslint": "^8.44.2",
    "@types/jest": "^29.5.4",
    "axios": "^1.6.3",
    "buffer": "^6.0.3",
    "clsx": "^2.0.0",
    "crypto-js": "^4.1.1",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "file-saver": "^2.0.5",
    "html5-qrcode": "^2.3.8",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43"
  },
  "devDependencies": {
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "cypress": "^13.1.0",
    "eslint": "^8.48.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "react-beautiful-dnd": "^13.1.1",
    "react-color": "^2.19.3",
    "react-countdown-circle-timer": "^3.2.1",
    "react-router-dom": "^6.15.0",
    "react-toastify": "^9.1.3",
    "serve": "^14.2.1",
    "start-server-and-test": "^2.0.0",
    "typescript": "^5.2.2",
    "vite": "^4.4.9"
  }
}
